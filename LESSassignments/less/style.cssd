  .bottom-image-figure {
    width: 95.7%;
  }
}
figcaption {
  font-family: sans-serif;
  font-size: 14px;
  color: #585858;
  text-align: center;
}
::-webkit-input-placeholder {
  color: #686868;
}
:-moz-placeholder {
  /* Firefox 18- */
  color: #686868;
}
::-moz-placeholder {
  /* Firefox 19+ */
  color: #686868;
}
:-ms-input-placeholder {
  color: #686868;
}
.disclaimertext {
  clear: both;
  width: 100%;
