/* ICON CSS */
@font-face {
  font-family: 'icomoon';
  src: url('fonts/icomoon.eot?uzepzs');
  src: url('fonts/icomoon.eot?uzepzs#iefix') format('embedded-opentype'), url('fonts/icomoon.ttf?uzepzs') format('truetype'), url('fonts/icomoon.woff?uzepzs') format('woff'), url('fonts/icomoon.svg?uzepzs#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}
[class^="icon-"],
[class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  font-size: 1.5em;
  color: #919191;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-arrows_plus:before {
  content: "\e900";
}
.icon-arrows_question:before {
  content: "\e901";
}
.icon-arrows_rotate_anti_dashed:before {
  content: "\e902";
}
