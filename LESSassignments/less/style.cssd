  font-family: sans-serif;
  font-size: 12px;
  text-align: center;
}
.disclaimertext p {
  padding: 2px 0px;
  margin: 0px;
}
.icon {
  display: inline-block;
  width: 3em;
  height: 2em;
  fill: currentColor;
  color: #999;
}
.icon-container {
  width: 142px;
  float: right;
  padding: 12px 0px;
}
@media only screen and (min-width: 320px) and (max-width: 500px) {
